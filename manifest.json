{
  "manifest_version": 2,
  "name": "Startup Tabs by Time",
  "description": "This extension allows you to set the startup tabs for specified time periods",
  "version": "1.0",
  
  "permissions": [
    "storage",
    "tabs"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "options_page": "options.html",
  
  
    
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  
  "chrome_settings_overrides": {
    "startup_pages": ["http://www.google.co.nz"]
  }
  
  
  
}