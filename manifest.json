{
  "manifest_version": 2,
  "name": "Startup Tabs by Time",
  "description": "This extension allows you to set the startup tabs for specified time periods",
  "version": "1.0",
  
  "permissions": [
    "tabs",
    "storage",
    "http://*/*", 
    "https://*/*"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "options_page": "options.html",
  
  
    
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Active Period"
  },
  /* "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ], */
  
  "chrome_settings_overrides": {
    "startup_pages": ["http://chrome-extension://agmmpomnnnffommpckgoppknbakkaapp/startPage.html"]
  }
  
  
  
}